{
    "name": "backend",
    "version": "1.0.0",
    "description": "backend is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
        "setup": "npm install && npm update",
        "tsc": "tsc",
        "build": "npm run tsc",
        "start": "ts-node-dev --transpileOnly ./src/server.ts",
        "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
        "prod": "tsc && node ./build/src/server.js"
    },
    "keywords": [
        "express",
        "backend",
        "backend"
    ],
    "license": "UNLICENSE",
    "dependencies": {
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-custom-error": "^1.8.4",
        "has-keys": "^0.1.0",
        "helmet": "^3.21.2",
        "http-status": "^1.3.2",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "morgan": "^1.9.1",
        "mysqlm": "^2.0.3",
        "rotating-file-stream": "^1.4.6",
        "ts-node-dev": "^1.0.0-pre.43"
    },
    "devDependencies": {
        "@types/express": "^5.0.1",
        "@types/node": "^22.13.14",
        "nodemon": "^3.1.9",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2"
    }
}
